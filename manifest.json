{
  "name": "YAML Diff Viewer",
  "short_name": "YAML Diff",
  "start_url": "./app.html",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#ffffff",
  "icons": [
    {
      "src": "icon.png",
      "sizes": "192x192",
      "type": "image/png"
    }
  ],
  "file_handlers": [
    {
      "action": "./app.html",
      "accept": {
        "text/yaml": [".yaml", ".yml"],
        "application/x-yaml": [".yaml", ".yml"]
      }
    }
  ]
}